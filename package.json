    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^2.6.0",
    "webpack-blocks-happypack": "^0.1.3",
    "webpack-blocks-split-vendor": "^0.2.0",
    "webpack-dev-server": "^2.3.0"
  }
